{
  "version": 2,
  "builds": [
      { "src": "index.js",
          "use": "@now/node-server"
          }
      ],
  "routes": [
        {
          "src": "/robot",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/users",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/robot",
          "dest": "/index.js",
          "methods": ["POST"]
        }
        ,
        {
          "src": "/robot",
          "dest": "/index.js",
          "methods": ["DELETE"]
        }
      ]
}
